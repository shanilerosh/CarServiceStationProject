	<subDataset name="ItemListing" uuid="695fc1dc-23a5-4136-892c-fecd7b367526">
		<parameter name="oidValueForTable" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     OrderDetail.`quantity` AS OrderDetail_quantity,
     OrderDetail.`itemPrice` AS OrderDetail_itemPrice,
     Item.`name` AS Item_name,
     Item.`unitprice` AS Item_unitprice,
     Item.`model` AS Item_model,
     OrderDetail.`quantity`*OrderDetail.`itemPrice` AS Total
FROM
     `Item` Item INNER JOIN `OrderDetail` OrderDetail ON Item.`iid` = OrderDetail.`iid`
WHERE 
OrderDetail.`oid`=$P{oidValueForTable};]]>
		</queryString>
		<field name="OrderDetail_quantity" class="java.lang.Double"/>
		<field name="OrderDetail_itemPrice" class="java.lang.Double"/>
		<field name="Item_name" class="java.lang.String"/>
		<field name="Item_unitprice" class="java.lang.Double"/>
		<field name="Item_model" class="java.lang.String"/>
		<field name="Total" class="java.lang.Double"/>
	</subDataset>
	<parameter name="PaymentID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     Payment.`pid` AS Payment_pid,
     Payment.`amount` AS Payment_amount,
     Payment.`pType` AS Payment_pType,
     Payment.`dateOfPayment` AS Payment_dateOfPayment,
     Payment.`supplierInvoiceNumber` AS Payment_supplierInvoiceNumber,
     Supplier.`name` AS Supplier_name,
     Supplier.`mobile` AS Supplier_mobile,
     Supplier.`email` AS Supplier_email
FROM
     `Supplier` Supplier INNER JOIN `Payment` Payment ON Supplier.`sid` = Payment.`sid`
WHERE
     Payment.`pid` = $P{PaymentID};]]>
	</queryString>
